package lecture_9_part2;

public class Ex03 {


	public static void main(String[] args) {
		Ex03 ex03=new Ex03();
		ThreadNotify t1=new ThreadNotify(ex03);
		t1.start();
		
		synchronized(ex03) {
			try {
				Thread.sleep(1000);
				ex03.notify();
			}catch(InterruptedException e) {
				e.printStackTrace();
			}
		}

	}
}



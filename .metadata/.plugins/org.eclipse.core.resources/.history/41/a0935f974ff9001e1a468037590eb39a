package lecture_9_part2;
public class Ex02{
	

	public static void main(String[] args) {
		Ex02 ex02=new Ex02();
		ThreadWait t1=new ThreadWait(ex02);
		t1.start();
		
		synchronized(ex02) {
			try {
				Thread.sleep(1000);
				ex02.notify();
			}catch(InterruptedException e) {
				e.printStackTrace();
			}
		}

	}
}

package ClassTest;



public class PartTimeEmployee extends Employee{
	private int OTHrs;
	
	public PartTimeEmployee(String name,double salary,int OTHrs) {
		super(name, salary);
		this.OTHrs=OTHrs;
	}
		
	public void displayEmpInfo() {
		System.out.println("Employee's name is:"+name+"\nSalary is:"+salary+"OT hours are:"+OTHrs);
	
	}

}
